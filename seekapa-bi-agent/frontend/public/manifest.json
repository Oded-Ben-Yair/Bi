{
  "name": "CEO Dashboard - Seekapa BI Agent",
  "short_name": "CEO Dashboard",
  "description": "Executive Intelligence Platform for real-time business insights and analytics",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#FFF7ED",
  "theme_color": "#F59E0B",
  "orientation": "any",
  "scope": "/",
  "lang": "en",
  "dir": "ltr",
  "categories": ["business", "productivity", "analytics"],
