    {
      "src": "/icon-maskable-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ],
  "shortcuts": [
    {
      "name": "Financial Dashboard",
      "short_name": "Financial",
      "description": "View financial performance metrics",
      "url": "/dashboard?view=financial",
      "icons": [
        {
          "src": "/shortcut-financial.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Analytics",
      "short_name": "Analytics",
      "description": "Predictive analytics and forecasts",
      "url": "/dashboard?view=analytics",
      "icons": [
        {
          "src": "/shortcut-analytics.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Alerts",
      "short_name": "Alerts",
      "description": "View critical alerts and anomalies",
      "url": "/dashboard?view=predictions",
      "icons": [
        {
          "src": "/shortcut-alerts.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  "related_applications": [
    {
      "platform": "webapp",
      "url": "https://dashboard.seekapa.com/manifest.json"
    }
  ],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "protocol_handlers": [
    {
      "protocol": "seekapa",
      "url": "/dashboard?action=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/dashboard/import",
      "accept": {
        "application/json": [".json"],
        "text/csv": [".csv"],
        "application/vnd.ms-excel": [".xls", ".xlsx"]
      },
      "icons": [
        {
          "src": "/file-handler-icon.png",
          "sizes": "256x256",
          "type": "image/png"
        }
      ],
      "launch_type": "single-client"
    }
  ]
}